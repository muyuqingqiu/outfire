@charset "utf-8";
/* CSS Document */



/*common  start*/
*{ 
	padding:0;
 	margin:0;
}
body{
	 font-family:Arial,Helvetica,sans-serif;
	 color:white;
	 height:100%;	 
}
li{ 
	list-style:none;
}
img{ 
	border:none; 
	vertical-align:top;
}
a{ 
	text-decoration:none;
	color:#fff;
}
i,em{ 
	font-style:normal;
}
.clear{ 
	zoom:1;
}
.clear:after{ 
	content:""; 
	display:block; 
	clear:both;
}
/*common  end*/

/*header  start*/
#header{ 
	width:100%; 
	height:704px;
	background:#CCC;
	position:relative;
}
#header_title{ 
	position:fixed;
	left:0;
	top:0;
	width:100%; 
	height:50px;
	background:#000;
	z-index:9999; 
}

/*header_nav start*/
#header_nav{
	position:fixed;
	left:0;
	top:0;
	height:50px;
	padding-left:260px;
}
#header_nav li{
	float:left;
}
#header_nav li a{
	display:block;
	height:100%;
	line-height:50px;
	padding:0 16px;
	color:#b7b7b7;
	font-size:12px;
	font-weight:bold;
	transition:.5s;
}
#header_nav li a:hover{
	color:white;
}
/*header_nav end*/

/*header_tips start*/
#header_tips{
	position:absolute;
	right:100px;
	padding-top:13px;
	z-index:9999;
}
#header_tips li{
	float:left;
}
#header_tips li a{
	display:block;
	width:23px;
	height:23px;
	background:url(../img/social-icons-small.png) no-repeat;
	transition:.5s;
}
#header_tips li:nth-of-type(1) a{
	background-position:0 0;
}
#header_tips li:nth-of-type(2) a{
	background-position:-23px 0;
}
#header_tips li:nth-of-type(3) a{
	background-position:-46px 0;
}
#header_tips li:nth-of-type(4) a{
	background-position:-69px 0;
}
#header_tips li:nth-of-type(5) a{
	background-position:-92px 0;
}

#header_tips li:nth-of-type(1) a:hover{
	background-position:0 -23px;	
}
#header_tips li:nth-of-type(2) a:hover{
	background-position:-23px -23px;	
}
#header_tips li:nth-of-type(3) a:hover{
	background-position:-46px -23px;	
}
#header_tips li:nth-of-type(4) a:hover{
	background-position:-69px -23px;	
}
#header_tips li:nth-of-type(5) a:hover{
	background-position:-92px -23px;	
}
/*header_tips end*/

/*keyboard start*/
#keyboard{ 
	width:34px; 
	height:32px;
	position:absolute;
	right:10px;
	top:7px;
	cursor:pointer;
	background:url(../img/keyboard-hint.png) no-repeat 0 0;
	transition:.5s;
	z-index:9999;
}
#keyboard .keyboard_tips{
	width:256px;
	height:69px;
	position:absolute;
	right:27px;
	top:24px;
	display:none;
	z-index:9999;
}
#keyboard .keyboard_tips .keyboard_bgLeft{
	width:31px;
	height:69px;
	display:block;
	float:right;
	background:url(../img/hint-left-bg.png) no-repeat;
}
#keyboard .keyboard_tips .keyboard_bgCenter{
	width:190px;
	height:69px;
	display:block;
	float:right;
	padding-top:20px;
	text-align:center;
	font-size:12px;
	font-weight:bold;
	color:#b7b7b7;
	background:url(../img/hint-center-bg.png) repeat-x;
}
#keyboard .keyboard_tips .keyboard_bgRight{
	width:35px;
	height:69px;
	display:block;
	float:right;
	background:url(../img/hint-right-bg.png) no-repeat;
}
#keyboard:hover{ 
	background-position:0 -32px;
}
#keyboard:hover .keyboard_tips{
	display:block;
}
/*keyboard end*/

/*header_left start*/
#header_left{
	width:370px;
	height:530px;
	background:white;
	position:fixed;
	top:-180px;
	left:-200px;
	transform:rotate(32deg);
	z-index:10000;
	box-shadow:0 0 10px 10px rgba(0,0,0,0.2);
}
#header_left .header_left_content{
	position:absolute;
	top:125px;
	right:0;
	transform:rotate(-32deg);
	padding:20px 0 0 20px;
}
#header_left .header_left_content li{
	float:left;
	padding:0 10px;
}
#header_left .header_left_content li a{
	color:#7f7f7f;
	font-size:8px;
	height:12px;
	line-height:12px;
	display:block;
	position:relative;
}
#header_left .header_left_content li a span{
	display:inline-block;
	width:8px;
	height:12px;
	background:url(../img/download-pdf.png) no-repeat 0 0;
	padding-right:5px;
	position:absolute;
	top:-1px;
	left:-10px;
}
#header_left .header_left_content li a:hover{
	color:#de3068;
}
#header_left .header_left_content li a:hover span{
	background-position:left -12px;
}
#header_left .header_left_logo a{
	display:block;
	width:160px;
	height:55px;
	position:absolute;
	right:0;
	top:200px;
	background:url(../img/logo_en.png) no-repeat;
	transform:rotate(-32deg);
}
